#
CONFIGVALUE=""

assign_variables()
{

  local EXT
  EXT=$1
  if [ "$EXT" = "x86Os" ]; then
    CONFIGVALUE=$CONFIGx86Os
  elif [ "$EXT" = "x86" ]; then
    CONFIGVALUE=$CONFIGx86
  elif [ "$EXT" = "x86mmx" ]; then
    CONFIGVALUE=$CONFIGx86mmx
  elif [ "$EXT" = "x86mmxOs" ]; then
    CONFIGVALUE=$CONFIGx86mmxOs
  elif [ "$EXT" = "k6p3" ]; then
    CONFIGVALUE=$CONFIGk6p3
  elif [ "$EXT" = "k7p4" ]; then
    CONFIGVALUE=$CONFIGk7p4
  elif [ "$EXT" = "k8p5" ]; then
    CONFIGVALUE=$CONFIGk8p5
  elif [ "$EXT" = "k9c2" ]; then
    CONFIGVALUE=$CONFIGk9c2
  elif [ "$EXT" = "atom" ]; then
    CONFIGVALUE=$CONFIGatom
  elif [ "$EXT" = "corex" ]; then
    CONFIGVALUE=$CONFIGcorex
  elif [ "$EXT" = "coreavx" ]; then
    CONFIGVALUE=$CONFIGcoreavx
  elif [ "$EXT" = "k16" ]; then
    CONFIGVALUE=$CONFIGk16
  elif [ "$EXT" = "k15" ]; then
    CONFIGVALUE=$CONFIGk15
  elif [ "$EXT" = "k14" ]; then
    CONFIGVALUE=$CONFIGk14
  elif [ "$EXT" = "k10" ]; then
    CONFIGVALUE=$CONFIGk10
  elif [ "$EXT" = "k9" ]; then
    CONFIGVALUE=$CONFIGk9
  elif [ "$EXT" = "k8" ]; then
    CONFIGVALUE=$CONFIGk8
  elif [ "$EXT" = "k7" ]; then
    CONFIGVALUE=$CONFIGk7
  elif [ "$EXT" = "k6" ]; then
    CONFIGVALUE=$CONFIGk6
  elif [ "$EXT" = "k5" ]; then
    CONFIGVALUE=$CONFIGk5
  elif [ "$EXT" = "k5Os" ]; then
    CONFIGVALUE=$CONFIGk5Os
  elif [ "$EXT" = "debug" ]; then
    CONFIGVALUE=$CONFIGdebug
  elif [ "$EXT" = "init" ]; then
    CONFIGVALUE=$CONFIGinit
  elif [ "$EXT" = "soft" ]; then
    CONFIGVALUE=$CONFIGsoft

  fi

}

